{
  "name": "@hyperbolajs/nestjs-kit",
  "version": "1.0.1",
  "description": "NestJS utils kit",
  "author": {
    "name": "wwwatson",
    "email": "tyanbiao@gmail.com",
    "url": "https://tangyanbiao.com"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"lib/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "hashids": "^2.2.10"
  },
  "devDependencies": {
    "rxjs": "^7.2.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "@nestjs/swagger": "^6.1.2",
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@hyperbolajs/eslint-config": "^1.1.2",
    "@hyperbolajs/prettier-config": "^1.0.0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.4",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.2",
    "prettier": "^2.7.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "prettier": "@hyperbolajs/prettier-config",
  "eslintConfig": {
    "extends": "@hyperbolajs/eslint-config/ts-node"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tyanbiao/nestjs-kit.git"
  },
  "peerDependencies": {
    "@nestjs/common": ">=9.0.0",
    "@nestjs/core": ">=9.0.0",
    "class-transformer": ">=0.5.0",
    "class-validator": ">=0.13.0",
    "@nestjs/swagger": ">=6.0.0"
  }
}
